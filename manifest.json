{
    "name": "Tab Saver",
    "description": "Remembers tabs so you can reopen them later",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "service_worker/background.js"
    },
    "icons": {
        "128": "icons/icon500.png"
    }
  }